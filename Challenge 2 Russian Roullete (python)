import random

def russian_roulette():
   
    
    bullet_position = random.randint(1, 6) // showing the exact principal
    print("Spinning the gun...")
    trigger_position = random.randint(1, 6)

    if bullet_position == trigger_position:
        print("Lmao! .. You're ded...")
    else:
        print("Nigga .. you're lucky...")

if __name__ == "__main__":
    russian_roulette()
